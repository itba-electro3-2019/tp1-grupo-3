:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55886e5d3f00 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x55886e5f4490_0 .var/i "i", 31 0;
v0x55886e5f4570_0 .var "x", 3 0;
v0x55886e5f4630_0 .net "y", 3 0, v0x55886e5f4350_0;  1 drivers
S_0x55886e5d4080 .scope module, "testing" "Ej4" 2 5, 3 1 0, S_0x55886e5d3f00;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "x"
    .port_info 1 /OUTPUT 4 "y"
v0x55886e5d4270_0 .net "x", 3 0, v0x55886e5f4570_0;  1 drivers
v0x55886e5f4350_0 .var "y", 3 0;
E_0x55886e596c50 .event edge, v0x55886e5d4270_0;
    .scope S_0x55886e5d4080;
T_0 ;
    %wait E_0x55886e596c50;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55886e5f4350_0, 4, 1;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 2, 3;
    %inv;
    %and;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 3, 3;
    %inv;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 2, 3;
    %and;
    %or;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55886e5f4350_0, 4, 1;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 1, 2;
    %inv;
    %and;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 2, 3;
    %inv;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 1, 2;
    %and;
    %or;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55886e5f4350_0, 4, 1;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 1, 2;
    %inv;
    %and;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 0, 2;
    %inv;
    %load/vec4 v0x55886e5d4270_0;
    %parti/s 1, 1, 2;
    %and;
    %or;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55886e5f4350_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55886e5d3f00;
T_1 ;
    %vpi_call 2 7 "$display", "Entrada\011Salida" {0 0 0};
    %vpi_call 2 8 "$display", "Binario\011Gray" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55886e5f4490_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x55886e5f4490_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x55886e5f4490_0;
    %pad/s 4;
    %store/vec4 v0x55886e5f4570_0, 0, 4;
    %delay 1, 0;
    %vpi_call 2 11 "$display", "%b\011%b", v0x55886e5f4570_0, v0x55886e5f4630_0 {0 0 0};
    %load/vec4 v0x55886e5f4490_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55886e5f4490_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;

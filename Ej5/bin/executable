v0x560b57472f50_0 .var "out", 7 0;
v0x560b57473030_0 .var "temp", 3 0;
v0x560b57473160_0 .var/i "unit", 31 0;
E_0x560b57444ce0 .event edge, v0x560b5744f260_0;
    .scope S_0x560b57452e80;
T_0 ;
    %wait E_0x560b57444ce0;
    %load/vec4 v0x560b5744f260_0;
    %pad/u 32;
    %store/vec4 v0x560b5744f300_0, 0, 32;
    %load/vec4 v0x560b5744f300_0;
    %pushi/vec4 10, 0, 32;
    %mod/s;
    %store/vec4 v0x560b57473160_0, 0, 32;
    %load/vec4 v0x560b5744f300_0;
    %load/vec4 v0x560b57473160_0;
    %sub;
    %pushi/vec4 10, 0, 32;
    %div/s;
    %store/vec4 v0x560b57450500_0, 0, 32;
    %load/vec4 v0x560b57450500_0;
    %pad/s 4;
    %store/vec4 v0x560b57473030_0, 0, 4;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 0, 2;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 1, 2;
    %ix/load 4, 5, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 6, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %load/vec4 v0x560b57473160_0;
    %pad/s 4;
    %store/vec4 v0x560b57473030_0, 0, 4;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 0, 2;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 1, 2;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %load/vec4 v0x560b57473030_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x560b57472f50_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x560b57452c60;
T_1 ;
    %wait E_0x560b57445450;
    %load/vec4 v0x560b574732a0_0;
    %pad/u 32;
    %load/vec4 v0x560b57473380_0;
    %pad/u 32;
    %mul;
    %cmpi/u 99, 0, 32;
    %jmp/0xz  T_1.0, 5;
    %load/vec4 v0x560b574732a0_0;
    %pad/u 8;
    %load/vec4 v0x560b57473380_0;
    %pad/u 8;
    %mul;
    %store/vec4 v0x560b57473460_0, 0, 8;
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 3 15 "$display", "Input invalido, el siguiente resultado no tendr\303\241 significado." {0 0 0};
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x560b57452ae0;
T_2 ;
    %vpi_call 2 8 "$display", "Se mostrar\303\241 el valor de las entradas en decimal, y luego la salida en  bcd" {0 0 0};
    %vpi_call 2 9 "$display", "x1    x  x2    =  BCD" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x560b57473660_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x560b57473660_0;
    %cmpi/s 30, 0, 32;
    %jmp/0xz T_2.1, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x560b57473740_0, 0, 32;
T_2.2 ;
    %load/vec4 v0x560b57473740_0;
    %cmpi/s 11, 0, 32;
    %jmp/0xz T_2.3, 5;
    %load/vec4 v0x560b57473660_0;
    %pad/s 4;
    %store/vec4 v0x560b57473820_0, 0, 4;
    %load/vec4 v0x560b57473740_0;
    %pad/s 4;
    %store/vec4 v0x560b57473920_0, 0, 4;
    %delay 1, 0;
    %vpi_call 2 14 "$display", "%d\011%d\011%b", v0x560b57473820_0, v0x560b57473920_0, v0x560b574739f0_0 {0 0 0};
    %load/vec4 v0x560b57473740_0;
    %addi 1, 0, 32;
    %store/vec4 v0x560b57473740_0, 0, 32;

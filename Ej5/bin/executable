v0x560b57473460_0 .var "TOTAL", 7 0;
v0x560b57473530_0 .net "Y", 7 0, v0x560b57472f50_0;  alias, 1 drivers
E_0x560b57445450 .event edge, v0x560b57473380_0, v0x560b574732a0_0;
S_0x560b57452e80 .scope module, "mybcd" "Binary2BCD" 3 10, 4 1 0, S_0x560b57452c60;
